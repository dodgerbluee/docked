{
    "name": "docked",
    "version": "1.3.2",
    "description": "Docker app maintenance tool with Portainer integration",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm start",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "keywords": [
        "docker",
        "portainer",
        "updates"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.2",
        "semver": "^7.6.0"
    }
}
